<h2 class='text-center'>Add Room</h2>

<%= form_for @room do |f| %>
  <div class="row">
    <div class = "small-offset-3 small-6 columns">
      <%= f.label :title, 'Title', class: 'input-group-label' %>
      <%= f.text_field :title, autofocus: true %>
    </div>
    <div class = "small-offset-3 columns"></div>
  </div>

  <%= f.submit 'Add', class: 'button button-center' %>
<% end %>

<%= link_to 'Go Back', rooms_path, class: 'button' %>
